{
  "name": "briyanadityatama/LaravelPHP-roles",
  "description": "Powerful package for handling roles and permissions in Laravel. Supports Laravel 5.3, 5.4, and 5.5",
  "keywords": [
    "roles",
    "permissions",
    "acl",
    "levels",
    "laravel"
  ],
  "license": "MIT",
  "authors": [
    {
      "name": "Briyan Adityatama"
    }
  ],
  "require": {
    "php": ">=5.6.4",
    "laravel/framework": "5.3.*|5.4.*|5.5.*|5.6.*|5.7.*"
  },
  "autoload": {
    "psr-4": {
      "briyanadityatama\\LaravelPHPRoles\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "briyanadityatama\\LaravelPHPRoles\\RolesServiceProvider"
      ]
    }
  },
  "config": {
    "sort-packages": true
  }
}
